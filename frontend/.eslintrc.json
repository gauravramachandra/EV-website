{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "import/first": "error",
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index"
        ],
        "newlines-between": "always"
      }
    ]
  }
}
